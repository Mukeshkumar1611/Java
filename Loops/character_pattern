import java.util.Scanner;
public class char_pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner ms = new Scanner(System.in);
		System.out.print("Enter Rows: ");
		int n = ms.nextInt();
		for(int i = 0; i<n;i++) {
			char temp = (char)(i+65);
			for(int j=0;j<=i;j++) {
				System.out.print(temp);
				temp++;
			}
			System.out.println();
		}

	}

}
